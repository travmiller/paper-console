# Local test credentials/config for print snapshot rendering.
# Copy to `.env` and fill values as needed.

# News module (NewsAPI)
PC1_TEST_NEWS_API_KEY=

# Email module (IMAP)
PC1_TEST_EMAIL_USER=
PC1_TEST_EMAIL_APP_PASSWORD=
PC1_TEST_EMAIL_HOST=imap.gmail.com

# RSS module (comma-separated feed URLs)
PC1_TEST_RSS_FEEDS=https://feeds.bbci.co.uk/news/rss.xml,https://rss.nytimes.com/services/xml/rss/nyt/HomePage.xml

# Calendar module (single ICS URL)
PC1_TEST_CALENDAR_ICS_URL=

# Webhook module
PC1_TEST_WEBHOOK_URL=https://icanhazdadjoke.com/
PC1_TEST_WEBHOOK_METHOD=GET
PC1_TEST_WEBHOOK_JSON_PATH=joke
